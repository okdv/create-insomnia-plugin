{
  "name": "insomnia-plugin-template-simple",
  "version": "0.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --ignore-path .gitignore --write ."
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "env": {
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    }
  },
  "author": "Otho DuBoise",
  "license": "MIT"
}
